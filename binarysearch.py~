
def werk(arr,left,right,x):
    while right >= left:
        mid = left+(right-left)//2
        if int(arr[mid]) == x:
            offset = 0
            if (mid-offset) == 0:
                return 0
            while(int(arr[mid-offset]) == x):
                offset+=1
            return mid-offset+1
        if int(arr[mid]) > x:
            right = mid-1
        else:
            left = mid+1
    return -1


stringIn = input("")
xar = stringIn.split(" ")
n = int(xar[0])
q = int(xar[1])
arr = {};
stringIn = input("")
arr = stringIn.split(" ")
for i in range(0,q):
    query = int(input(""))
    print(werk(arr,0,n,query))

